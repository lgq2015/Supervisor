    {
      "name": "SupervisorLogger",
      "source_files": "Core/SupervisorLogger/Classes/**/*"
    },
    {
      "name": "NSLogSupervisor",
      "source_files": "Core/NSLogSupervisor/Classes/**/*",
      "dependencies": {
        "fishhook": [
          "~> 0.2"
        ]
      }
    }
  ]
}
