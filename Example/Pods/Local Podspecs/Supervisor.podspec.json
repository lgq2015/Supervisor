{
  "name": "Supervisor",
  "version": "0.1.0",
  "summary": "a monitor tool kit for iOS developer.",
  "description": "a monitor tool kit for iOS developer.",
  "homepage": "https://github.com/madaoCN/Supervisor",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
